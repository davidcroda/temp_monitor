<div>
  <canvas id="myChart"></canvas>
</div>
<hr />
<%= if @temp do %>
<h1>Current</h1>
<section>
  <p>
    <b>Temperature: </b><%= @temp.temperature %>
  </p>
  <p>
    <b>Humidity: </b><%= @temp.humidity %>
  </p>
  <p>
    <b>Last Update: </b><%= @temp.inserted_at %>
  </p>
</section>
<% end %>
